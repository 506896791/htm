{
  "name": "htm",
  "version": "1.0.0",
  "description": "The Tagged Template Literal syntax for Virtual DOM. Only browser-compatible syntax.",
  "main": "dist.umd.js",
  "module": "dist.mjs",
  "scripts": {
    "build": "microbundle src.js -f es,umd --no-sourcemap --target web && npm run -s build:preact",
    "build:preact": "microbundle src-preact.js -o dist-preact.js -f es,umd --no-sourcemap --target web",
    "test": "eslint src.js src-preact.js test.js && jest test.js"
  },
  "eslintConfig": {
    "extends": "developit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gist.github.com/ce062586e3fc7247dbb9ccc4f6acc4e1.git"
  },
  "keywords": [
    "Hyperscript Tagged Markup",
    "tagged template literals",
    "html",
    "htm",
    "preact",
    "lit"
  ],
  "author": "Jason Miller <jason@developit.ca>",
  "license": "Apache-2.0",
  "homepage": "https://gist.github.com/ce062586e3fc7247dbb9ccc4f6acc4e1",
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-developit": "^1.1.1",
    "jest": "^23.1.0",
    "microbundle": "^0.4.4",
    "preact": "^8.2.9"
  }
}
